def isprime(n):
    for i in range(2,n//2 + 1):
        if n % i == 0 :
               return False
            
    return True


# Returns a list of booleans 
def prime(a):
    lst = []
    for n in a:
         lst.append(isprime(n))   # Append true or false 
        
    return lst  # List of booleans 


